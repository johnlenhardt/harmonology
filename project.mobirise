          "headerAlign": "left",
          "header": "HARMONOLOGY",
          "subheader": "Barbershop Quartet",
          "media1": {
            "image": {
              "active": true,
              "src": "@PROJECT_PATH@/assets/images/dan-madsen-600x600.jpg",
              "class": "mbr-figure__img",
              "width": 600,
